{
  "name": "@capraconsulting/sentry-utils-js",
  "version": "2.2.0",
  "description": "Utils for monitoring webapps with Sentry",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && microbundle",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config ./.prettierrc --write 'src/**/*.ts'",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/capraconsulting/sentry-utils-js"
  },
  "homepage": "https://github.com/capraconsulting/sentry-utils-js",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/jest-each": "0.3.4",
    "jest": "24.9.0",
    "jest-each": "24.9.0",
    "microbundle": "0.11.0",
    "prettier": "1.18.2",
    "rimraf": "2.7.1",
    "ts-jest": "24.0.2",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.2"
  },
  "dependencies": {
    "@sentry/browser": "^5.4.0"
  }
}

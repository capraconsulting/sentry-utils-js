{
  "name": "@capraconsulting/sentry-utils-js",
  "version": "0.0.0",
  "description": "Utils for monitoring webapps with Sentry",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "rimraf es dist && rollup -c",
    "test": "jest --coverage src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/capraconsulting/sentry-utils-js"
  },
  "homepage": "https://github.com/capraconsulting/sentry-utils-js",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/jest": "26.0.20",
    "@types/jest-each": "0.3.4",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.7",
    "jest": "26.6.3",
    "jest-each": "26.6.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.36.1",
    "rollup-plugin-typescript2": "0.29.0",
    "semantic-release": "17.3.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@sentry/browser": "^5.21.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
